import math

def gcd(a, b):
    while b != 0:
        a, b = b, a % b
    return a

def pollards_rho(n):
    if n % 2 == 0:
        return 2
    x = 2; y = 2; d = 1
    f = lambda x: (x**2 + 1) % n
    while d == 1:
        x = f(x)
        y = f(f(y))
        d = gcd(abs(x-y), n)
    return d

def rsa_factor_ing(n):
    factor = pollards_rho(n)
    return factor, n // factor

print(rsa_factor_ing(2497885147362973))  # The expaected Output: (49979141, 49978553)